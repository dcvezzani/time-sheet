<h1>Listing time_sheet_entries</h1>

<table>
  <tr>
    <th>Starts at</th>
    <th>Ends at</th>
    <th>Starts at</th>
    <th>User</th>
    <th>Summary</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_sheet_entries.each do |time_sheet_entry| %>
  <tr>
    <td><%= time_sheet_entry.starts_at %></td>
    <td><%= time_sheet_entry.ends_at %></td>
    <td><%= time_sheet_entry.starts_at %></td>
    <td><%= time_sheet_entry.user_id %></td>
    <td><%= time_sheet_entry.summary %></td>
    <td><%= time_sheet_entry.content %></td>
    <td><%= link_to 'Show', time_sheet_entry %></td>
    <td><%= link_to 'Edit', edit_time_sheet_entry_path(time_sheet_entry) %></td>
    <td><%= link_to 'Destroy', time_sheet_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

